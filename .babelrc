{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "30",
          "ie": "8"
        },
        "useBuiltIns": "usage",
        "corejs": { "version": "3.2", "proposals": true }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": { "version": 3, "proposals": true }
      }
    ]
  ]
}